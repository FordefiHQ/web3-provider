{
    "entryPoints": ["src/index.ts"],
    "name": "Fordefi Web3 Provider Docs",
    "out": "docs/html",
    "theme": "default",
    "sort": ["kind", "source-order"],
    "kindSortOrder": ["Class", "Interface", "TypeAlias", "Enum", "Variable"],
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": false,
    "excludeInternal": true,
    "includeVersion": true,
    "disableSources": true,
    "excludeReferences": false,
    "hideGenerator": true
}
